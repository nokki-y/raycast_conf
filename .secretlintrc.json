{
  "rules": [
    {
      "id": "@secretlint/secretlint-rule-preset-recommend"
    }
  ],
  "ignoreFile": [".gitignore"],
  "allowMessageIds": [],
  "disabledMessages": [],
  "options": {
    "ignorePaths": [
      "**/node_modules/**",
      "**/dist/**",
      "**/build/**",
      "**/*.lock",
      "**/*.svg",
      "**/*.png",
      "**/*.jpg",
      "**/*.jpeg",
      "**/*.gif",
      ".git/**",
      ".secrets.baseline",
      ".gitleaks.toml",
      ".pre-commit-config.yaml"
    ]
  }
}
